<!DOCTYPE html>
<html lang="en">
  <%- include('include/_header') %>
<body><div class="w3-margin-top w3-display-topmiddle container-row">
    <select name="" id="selectCategory" class="w3-select w3-margin-right w3-border">
      <option value="/courses" selected>All</option>
      <% if(typeof category !== 'undefined') {%> <%for (let key in category) {%>
      <option value="<%=key%>"><%=key%></option>
      <% } %> <% } %>
    </select>
    <button id="clickMe" class="w3-button w3-blue">Run</button>
  </div>
  <div class=" w3-display-middle">
    <table class="w3-table w3-bordered w3-striped" id="coursesTable">
      <thead>
        <tr class="w3-red">
          <th></th>
          <th>id</th>
          <th>Course name</th>
          <th>Category name</th>
        </tr>
      </thead>
      <tbody id="coursesTableBody">
        <% if(typeof category !== 'undefined') {%>
        <%for (let key in category) {%>
        <%for(let i=0; i <category[key].length; i++) {%>
        <tr>
          <td><input type="checkbox" name="" id=""></td>
          <td><%=(category[key])[i].courseid%></td>
          <td><%=(category[key])[i].course%></td>
          <td class="category"><%=(category[key])[i].category%></td>
        </tr>
        <%}%>
        <%}%>
        <%}%>
      </tbody>
    </table>
  </div></body>

  </html>